        <Button x:Name="ButtonOpen"             Content="Open New"                  HorizontalAlignment="Left" Margin="91,61,0,0"       VerticalAlignment="Top" FontSize="20" Click="ButtonOpen_Click" Grid.Column="1" Height="31" Width="53"/>
        <Button x:Name="ButtonSavePdf"          Content="Save Template"             HorizontalAlignment="Left" Margin="183,251,0,0"     VerticalAlignment="Top" FontSize="20" Click="ButtonSave_Click" Grid.Column="1" Height="31" Width="45"/>
