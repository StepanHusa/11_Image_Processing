        <Button x:Name="ButtonRun"              Content="Run"                       HorizontalAlignment="Left" Margin="183,176,0,0"     VerticalAlignment="Top" FontSize="20" Click="ButtonRun_Click"   Grid.Column="1" Height="31" Width="45" />
        <Button x:Name="ButtonNew"              Content="Blank"                     HorizontalAlignment="Left" Margin="30,61,0,0"       VerticalAlignment="Top" FontSize="20" Click="ButtonNew_Click" Grid.Column="1" Height="31" Width="40"/>


    </Grid>
</Window>
